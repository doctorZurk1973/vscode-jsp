{
    "name": "vscode-jtl-jsp",
    "displayName": "Java Server Pages (JSP)",
    "description": "Java Template Language(JTL) JSP language support for Visual Studio Code, ported from TextMate's JSP bundle.",
    "version": "0.0.31",
    "publisher": "tlintrami",
    "repository": "https://github.com/doctorZurk1973/vscode-jsp.git",
    "icon": "ext-icon.png",
    "author": {
        "name": "Tommaso Lintrami",
        "url": "https://github.com/doctorZurk1973",
        "email": "tommaso.lintrami@dshare.com"
    },
    "engines": {
        "vscode": "1.78.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jsp",
                "aliases": [
                    "Java Server Pages",
                    "jsp",
                    "JSP",
                    "jstl",
                    "JSTL"
                ],
                "extensions": [
                    ".tag",
                    ".jsp",
                    ".jspf"
                ],
                "configuration": "./jsp-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jsp",
                "scopeName": "text.html.jsp",
                "path": "./syntaxes/jsp.tmLanguage.json",
                "embeddedLanguages": {
                    "source.css": "css",
                    "source.js": "javascript"
                }
            }
        ]
    }
}
